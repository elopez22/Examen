@{
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>

    <script src="@Url.Content("js/Controller/Busqueda.js")"></script>
}
<form>
    <section>
        <div class="row g-0">
            <div class="col-lg-12">
                <div class="align-self-center">
                    <div class="col-lg-6">
                        <div class="input-group mb-4">
                            <label for="txtramo">Ramo</label>
                            <input type="number" ID="txtramo" class="form-control" placeholder="buscar por clave" />
                            <input type="text" ID="txtramodesc" readonly  class="form-control" placeholder="Descripcion" />
                            @Html.ActionLink("Buscar en catálogo", "Index", "CatalogoRama")
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <label for="txtramo">Producto</label>
                        <div class="input-group mb-4">
                            <input type="number" ID="txtproducto" disabled class="form-control" placeholder="buscar por clave" />
                            <input type="text" ID="txtproductodesc" readonly class="form-control" placeholder="buscar" />
                            @Html.ActionLink("Buscar en catálogo", "Index", "CatalogoProd")
                        </div>
                    </div>






                </div>



            </div>
        </div>
    </section>
    <!-- ////////////////////////////////////////////////////////////////////////////-->
    <!-- BEGIN VENDOR JS-->

</form>

